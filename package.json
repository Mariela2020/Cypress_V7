{
  "name": "cypressqa",
  "version": "1.0.0",
  "description": "Cypress Automation",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:dashboard": "cypress run --browser edge --record --key 1f59ceae-3fd6-40e5-9073-5c8d1aba265d"
  },
  "keywords": [
    "Cypress",
    "CI-CD",
    "cucumber"
  ],
  "author": "Mariela",
  "license": "ISC",
  "devDependencies": {
    "cucumber-html-reporter": "^5.4.0",
    "cypress": "^7.0.0",
    "cypress-cucumber-preprocessor": "^4.0.3"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/integration/features/step_definitions/**/",
    "cucumberJson": {
        "generate": true,
        "outputFolder": "cypress/cucumber-report",
        "filePrefix": "",
        "fileSuffix": ".cucumber"
      }
    }
  }

