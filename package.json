{
  "name": "cypressqa",
  "version": "1.0.0",
  "description": "Cypress Automation",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run -- spec 'cypress/integration/test-newpage.js'",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:dashboard": "cypress run --browser chrome --spec 'cypress/integration/features/corredora.feature' --record --key 1f59ceae-3fd6-40e5-9073-5c8d1aba265d --reporter mochawesome",
    "save:results": "cypress run --spec 'cypress/integration/features/reporte.feature'"
  },
  "keywords": [
    "Cypress",
    "CI-CD",
    "cucumber"
  ],
  "author": "Mariela",
  "license": "ISC",
  "devDependencies": {
    "cucumber-html-reporter": "^5.4.0",
    "cypress": "^7.0.0",
    "cypress-cucumber-preprocessor": "^4.0.3"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/integration/features/step_definitions/**/",
    "cucumberJson": {
      "generate": false,
      "outputFolder": "cypress/cucumber-report",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "dependencies": {
    "mochawesome": "^6.2.2"
  }
}
